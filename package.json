{
  "name": "user-service",
  "version": "1.0.0",
  "description": "Microservice to Create, Read, Update and Delete Rural Producers.",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build && npx sequelize-cli db:migrate",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register .",
    "build": "tsc --p tsconfig.build.json",
    "pretty-quick": "npx pretty-quick --staged --verbose --pattern \"**/*.*(js|ts|json)\"",
    "prestart:dev": "npx sequelize-cli db:migrate",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts --trace-warnings",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "author": "Denis Slapelis",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.484.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.8",
    "@types/sequelize": "^4.28.19",
    "@types/winston": "^2.4.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
